{
  "name": "unplugin-sayable",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsdown",
    "prepack": "pnpm build"
  },
  "files": ["dist", "!dist/**/*.map"],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.mts",
      "default": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@sayable/babel-plugin": "workspace:^",
    "unplugin": "^2.3.10"
  }
}
