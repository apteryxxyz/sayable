{
  "name": "unplugin-sayable",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsdown",
    "prepack": "pnpm build"
  },
  "files": ["dist", "!dist/**/*.map"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "dependencies": {
    "@sayable/factory": "workspace:^",
    "unplugin": "^2.3.10"
  }
}
